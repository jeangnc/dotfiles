{
  "$schema": "https://json.schemastore.org/claude-code-settings.json",
  "permissions": {
    "allow": [
      "Bash(cat:*)",
      "Bash(curl:*)",
      "Bash(exit)",
      "Bash(find:*)",
      "Bash(grep:*)",
      "Bash(head:*)",
      "Bash(ls:*)",
      "Bash(lsof:*)",
      "Bash(mkdir:*)",
      "Bash(netstat:*)",
      "Bash(ps:*)",
      "Bash(pwd:*)",
      "Bash(tail:*)",
      "Glob",
      "Grep",
      "Read",
      "WebFetch",
      "WebSearch"
    ],
    "deny": [
      "Bash(sudo:*)"
    ],
    "defaultMode": "plan"
  },
  "enabledPlugins": {
    "gq-dev@gq-marketplace": true,
    "finance-tools@jeangnc-marketplace": true,
    "dev-tools@jeangnc-marketplace": true,
    "note-tools@jeangnc-marketplace": true
  },
  "alwaysThinkingEnabled": true
}
